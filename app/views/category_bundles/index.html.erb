<h1>Listing category_bundles</h1>

<table>
  <tr>
    <th>Cat</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @category_bundles.each do |category_bundle| %>
  <tr>
    <td><%= category_bundle.cat_id %></td>
    <td><%= category_bundle.user_id %></td>
    <td><%= link_to 'Show', category_bundle %></td>
    <td><%= link_to 'Edit', edit_category_bundle_path(category_bundle) %></td>
    <td><%= link_to 'Destroy', category_bundle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Category bundle', new_category_bundle_path %>
